{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc"
    ]
  },
  "source": {
    "include": [
      "./"
    ],
    "exclude": [
      "node_modules/",
      "src-front-end/",
      "docs/",
      ".config/",
      "public/"
    ]
  },
  "opts": {
    "encoding": "utf8",
    "destination": "docs/server/",
    "recurse": true
  }
}
