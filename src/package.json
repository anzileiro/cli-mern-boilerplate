{
  "name": "boilerplate",
  "version": "0.0.1",
  "private": false,
  "author": {
    "name": "boilerplate",
    "email": "boilerplate"
  },
  "scripts": {
    "start": "pm2 start .config/pm2.process.json && pm2 logs",
    "stop": "pm2 stop .config/pm2.process.json",
    "start:dev": "NODE_ENV=development npm run build:client && NODE_ENV=development npm run start:server",
    "start:server": "node ./bin/www",
    "build:client": "webpack --config .config/webpack.config.dev.js --progress --profile",
    "build:client:production": "webpack --config .config/webpack.config.prod.js --progress --profile",
    "build:production": "npm run build:client:production && npm test && npm run docs:client && npm run docs:server",
    "test:client": "karma start",
    "test:server": "mocha spec --colors --recursive --require babel-register",
    "test": "npm run test:server && npm run test:client",
    "logs": "pm2 logs",
    "docs:client": "./node_modules/.bin/jsdoc --configure .config/conf.jsdoc.client.json",
    "docs:server": "./node_modules/.bin/jsdoc --configure .config/conf.jsdoc.server.json"
  },
  "dependencies": {
    "body-parser": "~1.15.1",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.13.4",
    "immutable": "^3.8.1",
    "jade": "~1.11.0",
    "morgan": "~1.7.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.8.1",
    "redux": "^3.6.0",
    "redux-actions": "^0.12.0",
    "redux-immutable": "^3.0.8",
    "redux-logger": "^2.7.0",
    "redux-saga": "^0.12.0",
    "router": "^1.1.4",
    "serve-favicon": "~2.3.0",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.0",
    "winston": "^2.2.0"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-cli": "^6.16.0",
    "babel-core": "^6.17.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-plugin-webpack-loaders": "^0.7.1",
    "babel-polyfill": "^6.16.0",
    "babel-preset-airbnb": "^2.1.1",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-register": "^6.16.3",
    "babel-runtime": "^6.11.6",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.4.1",
    "eslint": "^3.8.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-loader": "^1.6.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "jsdoc": "^3.4.2",
    "json-loader": "^0.5.4",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-mocha": "^1.2.0",
    "karma-mocha-reporter": "^2.2.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mocha": "^3.1.2",
    "phantomjs-prebuilt": "^2.1.13",
    "react-addons-test-utils": "^15.3.2",
    "style-loader": "^0.13.1",
    "supertest": "^2.0.0"
  },
  "license": "MIT",
  "description": "MERN project"
}
